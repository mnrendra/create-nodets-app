{
  "name": "create-nodets-app",
  "version": "0.1.0",
  "description": "My initial `NodeJs` project template using `TypeScript`.",
  "main": "./build",
  "scripts": {
    "build": "rm -r ./build && tsc -p tsconfig.json",
    "start": "npm run build && node -r dotenv/config .",
    "dev": "nodemon -e ts --exec 'npm run start'",
    "prod": "npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mnrendra/create-nodets-app.git"
  },
  "keywords": [
    "create-nodets-app"
  ],
  "author": "@mnrendra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mnrendra/create-nodets-app/issues"
  },
  "homepage": "https://github.com/mnrendra/create-nodets-app#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "module-alias": "^2.2.2",
    "mongoose": "^6.4.4"
  },
  "_moduleAliases": {
    "@commons": "build/commons",
    "@config": "build/config",
    "@db": "build/db",
    "@lib": "build/lib",
    "@models": "build/models",
    "@routes": "build/routes",
    "@schemas": "build/schemas"
  }
}
